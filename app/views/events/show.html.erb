<h3><%= @event.name %></h3>
<p>Date: <%= @event.date %></p>
<p>Start Time: <%= @event.start_time %></p>
<p>End Time: <%= @event.end_time %></p>

<% if @event.workers %>
  <ul>
    <% @event.workers.each do |worker| %>
    <li>
      <%= "#{worker.first_name} #{worker.last_name}" %>
    </li>
    <% end %>
  </ul>
<% end %>
